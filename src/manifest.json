{
    "manifest_version": 2,
    "name": "Custom Code Insertion",
    "version": "1.0.0",
    "minimum_chrome_version":  "68",
    "description": "Run custom code in a webpage",
    "author": "Charlton Cheng",
    "permissions": [
        "storage",
        "<all_urls>"
    ],
    "icons": {
        "16": "common/icon_16.png",
        "32": "common/icon_32.png",
        "48": "common/icon_48.png",
        "128": "common/icon_128.png"
    },
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": [ "*://*/*" ],
            "js": [
                "common/app.min.js",
                "content-script/main.min.js"
            ]
        }
    ],
    "browser_action": {
        "default_title": "Custom Code Insertion",
        "default_popup": "popup/index.html",
        "default_icon": {
            "16": "common/icon_16.png",
            "24": "common/icon_24.png",
            "32": "common/icon_32.png"
        }
    },
    "web_accessible_resources":  [
        "common/app.min.css",
        "common/shl-app-icomoon.woff"
    ]
}
